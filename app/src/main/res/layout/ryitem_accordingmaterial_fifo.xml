<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_ll"
    android:layout_marginTop="@dimen/layout_10dp"
    android:paddingRight="@dimen/layout_10dp"
    android:paddingLeft="@dimen/layout_10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        style="@style/itemstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_locator_string"
            android:layout_width="@dimen/layout_70dp"
            android:layout_height="wrap_content"
            android:text="@string/locator"
            android:textSize="@dimen/textSize_14sp" />

        <TextView
            android:id="@+id/tv_locator"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:textSize="@dimen/textSize_14sp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_batch_date_string"
                android:layout_width="@dimen/layout_70dp"
                android:layout_height="wrap_content"
                android:text="@string/batch_date"
                android:textSize="@dimen/textSize_12sp" />

            <TextView
                android:id="@+id/tv_batch_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_12sp" />
        </LinearLayout>
     </LinearLayout>

    <View
        android:id="@+id/line_1"
        style="@style/ry_line_style" />

    <LinearLayout
        style="@style/itemstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_item_no_string"
            android:layout_width="@dimen/layout_70dp"
            android:layout_height="wrap_content"
            android:text="@string/item_no"
            android:textSize="@dimen/textSize_14sp" />

        <TextView
            android:id="@+id/tv_item_no"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSize_14sp" />
    </LinearLayout>

    <View
        android:id="@+id/line_2"
        style="@style/ry_line_style" />

    <LinearLayout
        style="@style/itemstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_rdna_num_string"
                android:layout_width="@dimen/layout_70dp"
                android:layout_height="wrap_content"
                android:text="@string/rdna_num"
                android:textSize="@dimen/textSize_12sp" />

            <TextView
                android:id="@+id/tv_rdna_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_12sp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_actual_yield_string"
                android:layout_width="@dimen/layout_70dp"
                android:layout_height="wrap_content"
                android:text="@string/actual_yield"
                android:textSize="@dimen/textSize_12sp" />

            <TextView
                android:id="@+id/tv_actual_yield"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_12sp" />
        </LinearLayout>
    </LinearLayout>
    <View style="@style/fifo_line_style" />
</LinearLayout>
